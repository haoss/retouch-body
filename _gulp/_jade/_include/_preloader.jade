mixin mobileNav
  .mobile-nav
    button(type="button").header__mobile.header__mobile--close
      i.fa.fa-remove(aria-hidden='true')
    .mobile-nav__wrapper
      nav.mobile-nav__nav
        ul
          li
            a(href="#!") Категории
          li
            a(href="#!") Статьи
          li
            a(href="#!") Связаться с нами

      .header__lang
        .header__lang-current
          span 
          i.fa.fa-angle-down
        ul
          li.active: span Рус
          li: a(href="#!") Eng
          li: a(href="#!") Укр
          li: a(href="#!") Pl
